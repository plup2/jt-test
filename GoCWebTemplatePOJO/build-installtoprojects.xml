                <exclude name="**/*.java"/>
            </fileset>
        </copy>
    </target>
    <target name="clean">
        <delete dir="${compile.targetdir}"/>
    </target>
	
    <target depends="clean" name="cleanall"/>
    <target depends="clean,build-subprojects,build-project" name="build"/>
    <target name="build-subprojects"/>
	
    <target depends="init" name="build-project">
        <echo message="${ant.project.name}: ${ant.file}"/>
        <javac debug="${debug.enabled}" debuglevel="${debuglevel}" destdir="${compile.targetdir}" includeantruntime="false" source="${source}" target="${target}">
            <src path="src"/>
            <classpath refid="GoCWebTemplatePOJO.classpath"/>
        </javac>
    </target>	
	
