<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    <!--  ============================================================= -->
    <!--  this declares the CDN configuration properties file, please do 
    	  NOT remove or modify -->
	<application>
		<resource-bundle>
			<base-name>goc.webtemplate.global.config.cdn</base-name>
			<var>cdn</var>
		</resource-bundle>
	</application>
	<!--  ============================================================= -->
	
	<!--  ============================================================= -->
	<!--  The following 2 beans declaration are used by the master template 
	      pages, please do NOT remove or modify -->
	<managed-bean>
		<managed-bean-name>defaulttemplatejsfbean</managed-bean-name>
		<managed-bean-class>goc.webtemplate.component.jsf.DefaultTemplateCoreBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>applicationscopebean</managed-bean-name>
		<managed-bean-class>goc.webtemplate.component.jsf.MasterApplicationBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<!--  ============================================================= -->
	
	<!--  ============================================================= -->
	<!--  The following set of bean declarations are for the sample pages 
	      only, you can remove them at any time if the sample pages are 
	      no longer required -->
	<managed-bean>
		<managed-bean-name>indexbean</managed-bean-name>
		<managed-bean-class>goc.webtemplate.jsf.samplebeans.IndexBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>addjsandcssfilessamplebean</managed-bean-name>
		<managed-bean-class>goc.webtemplate.jsf.samplebeans.AddJsAndCssFilesSample</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>basesettingssamplebean</managed-bean-name>
		<managed-bean-class>goc.webtemplate.jsf.samplebeans.BaseSettingsSample</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>breadcrumbsamplebean</managed-bean-name>
		<managed-bean-class>goc.webtemplate.jsf.samplebeans.BreadcrumbSample</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>extendedbasepagesamplebean</managed-bean-name>
		<managed-bean-class>goc.webtemplate.jsf.samplebeans.ExtendedBasePageSample</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>feedbackandsharethispagesamplebean</managed-bean-name>
		<managed-bean-class>goc.webtemplate.jsf.samplebeans.FeedbackAndShareThisPageSample</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>footerlinkssamplebean</managed-bean-name>
		<managed-bean-class>goc.webtemplate.jsf.samplebeans.FooterLinksSample</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>leavingsecuresitesamplebean</managed-bean-name>
		<managed-bean-class>goc.webtemplate.jsf.samplebeans.LeavingSecureSiteSample</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>leftsidemenusamplebean</managed-bean-name>
		<managed-bean-class>goc.webtemplate.jsf.samplebeans.LeftSideMenuSample</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>nestedchildpagesamplebean</managed-bean-name>
		<managed-bean-class>goc.webtemplate.jsf.samplebeans.NestedChildPageSample</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>sessiontimeoutsamplebean</managed-bean-name>
		<managed-bean-class>goc.webtemplate.jsf.samplebeans.SessionTimeoutSample</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>transactionsamplebean</managed-bean-name>
		<managed-bean-class>goc.webtemplate.jsf.samplebeans.TransactionSample</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
    <managed-bean>
        <managed-bean-name>applicationsamplebean</managed-bean-name>
        <managed-bean-class>goc.webtemplate.jsf.samplebeans.ApplicationSample</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
	<managed-bean>
		<managed-bean-name>gcintranetsamplebean</managed-bean-name>
		<managed-bean-class>goc.webtemplate.jsf.samplebeans.IntranetSample</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<!--  ============================================================= -->
</faces-config>
